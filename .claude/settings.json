{
  "permissions": {
    "allow": [
      "Bash(uv run *)",
      "Bash(uv sync *)",
      "Bash(uv add *)",
      "Bash(mise run *)",
      "Bash(mise tasks *)",
      "Bash(ruff *)",
      "Bash(git status)",
      "Bash(git status *)",
      "Bash(git diff)",
      "Bash(git diff *)",
      "Bash(git log *)",
      "Bash(git add *)",
      "Bash(git commit *)",
      "Bash(git branch *)",
      "Bash(git checkout *)",
      "Bash(git show *)",
      "Bash(git stash *)",
      "Bash(pytest *)",
      "Bash(python -c *)",
      "Bash(python -m *)",
      "Bash(ls)",
      "Bash(ls *)",
      "Bash(cat *)",
      "Bash(head *)",
      "Bash(tail *)",
      "Bash(wc *)",
      "Bash(gh pr *)",
      "Bash(gh issue *)",
      "Bash(gh api *)",
      "Bash(gh repo *)"
    ],
    "deny": [
      "Read(.env)",
      "Read(.env.*)",
      "Read(.mise.local.toml)",
      "Read(**/*.key)",
      "Read(**/secrets/**)",
      "Read(**/*credential*)",
      "Bash(rm -rf *)",
      "Bash(sudo *)",
      "Bash(curl * | *)",
      "Bash(pip install *)",
      "Bash(pip uninstall *)",
      "Bash(git push --force *)",
      "Bash(git push -f *)",
      "Bash(git reset --hard *)",
      "Bash(git clean -f *)",
      "Bash(pyenv *)",
      "Bash(python3.14 *)",
      "Bash(python3.12 *)",
      "Bash(python3.11 *)"
    ],
    "additionalDirectories": []
  },
  "env": {
    "PYTHONDONTWRITEBYTECODE": "1"
  },
  "extraKnownMarketplaces": {
    "cc-skills": {
      "source": {
        "source": "github",
        "repo": "terrylica/cc-skills"
      }
    }
  },
  "respectGitignore": true
}
